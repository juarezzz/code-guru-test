{
  "name": "cdk-polytag",
  "version": "1.0.0",
  "sideEffects": false,
  "bin": {
    "project": "build/project.js"
  },
  "scripts": {
    "build": "ts-node -r tsconfig-paths/register build/project.ts",
    "cdk": "cdk",
    "cfn:build:windows:cfn-origin": "tsc ./src/stacks/web-stack/lambdas/cloudfront/analytics-web-origin-request/handler.ts --moduleResolution node",
    "cfn:build:windows1": "tsc ./src/polytag-stack/lambdas/cloudfront/resolver-viewer-request/handler.ts --target es5 --outfile ./src/polytag-stack/lambdas/cloudfront/resolver-viewer-request/handler.js",
    "cfn:build-edge": "tsc ./src/analytics-edge-stack/lambdas/cloudfront/edge-function/handler.ts --moduleResolution node",
    "cfn:build": "tsc ./src/polytag-stack/lambdas/cloudfront/**/handler.ts --target es5",
    "deploy": "ts-node -r tsconfig-paths/register ./build/deploy.ts",
    "dev:windows": "set DEBUG=app,error & tsnd -r tsconfig-paths/register --respawn --clear --notify server/index.ts",
    "dev": "DEBUG=app,error tsnd -r tsconfig-paths/register --respawn --clear --notify server/index.ts",
    "dev:stg": "DEBUG=app,error CONFIG_PATH=env/.env.stg tsnd -r tsconfig-paths/register --respawn --clear --notify server/index.ts",
    "dev:preprod": "DEBUG=app,error CONFIG_PATH=env/.env.preprod tsnd -r tsconfig-paths/register --respawn --clear --notify server/index.ts",
    "lint": "tsc --noEmit && eslint --fix .",
    "test:watch": "jest --colors --watch",
    "test": "jest --coverage --json --colors --verbose --outputFile=output.json",
    "scripts": "ts-node -r tsconfig-paths/register scripts/index.ts"
  },
